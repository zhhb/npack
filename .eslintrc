{
  'extends': 'airbnb',
  'ecmaFeatures': {
    'jsx': true,
  },
  'rules': {
      'comma-dangle': [1, 'always-inline'],
	  'id-length': 0,
	  'indent': [1,2, {SwitchCase: 0}],
	  'no-console': 0,
	  'no-shadow': 0,
	  'no-new-func': 0,
	  'one-var': 0,
	  'object-curly-spacing': 0,
	  'no-nested-ternary': 0,
	  'no-param-reassign': 0,
	  'prefer-template': 0, 
	  'prefer-arrow-callback':0,
	  'dot-notation': 0,
	  'max-len':0,
	  'react/jsx-no-kind': 0,
	  'react/jsx-boolean-value': 0,
	  'react/jsx-closing-bracket-location': [2, 'after-props'],
	  'react/no-multi-comp': 0
  },
  'globals': {
    'Prel': false,
	'define': false,
	'NDEBUG': false,
	'MODULE_VERSIONS': false,
	'VERSION': false,
	'RUNTIME_TARGET': false,
	'describe': false,
	'it':false,
	'afterEach': false,
	'__webpack_public_path__': true
  }
}
